--python-kinds=-iv

--langdef=xslt
--langmap=xslt:.xsl
--regex-xslt=/template+match="([^"]+)"( +mode="([^"]+)")?/\1 \2/m,match/
--regex-xslt=/template+name="([^"]+)"( +mode="([^"]+)")?/\1 \2/n,name/
--regex-xslt=/apply-templates+select="([^"]{2,})"( +mode="([^"]+)")?/\1 \2/a,apply/
--regex-xslt=/call-templates+select="([^"]+)"( +mode="([^"]+)")?/\1 \2/c,call/

--langdef=markdown
--langmap=markdown:.md.markdown.mdown.mkd.mkdn
--regex-markdown=/^#[\t]+(.*)/\1/h,heading1/
--regex-markdown=/^##[\t]+(.*)/\1/h,heading2/
--regex-markdown=/^###[\t]+(.*)/\1/h,heading3/


--langdef=rst
--langmap=rst:.rst
--regex-rst=/^#+$/##########/h,headers/
--regex-rst=/^=+$/==========/h,headers/
--regex-rst=/^-+$/----------/h,headers/
--regex-rst=/^\^+$/^^^^^^^^^^/h,headers/
--regex-rst=/^\.{2}[\t]+([^:]+):$/\1/r,references/


--langdef=js
--langmap=js:.js
--regex-js=/([A-Za-z0-9._$]+)\s*=\s*function\s+/\1/f,function/
--regex-js=/function\s+([A-Za-z0-9._$]+)\s*\(/\1/f,function/
--regex-js=/var\s+([A-Za-z0-9._$]+)\s*=/\1/v,variable/
--regex-js=/([A-Za-z0-9._$]+)\s*:\s*function\s+/\1/m,method/
--regex-js=/([A-Za-z0-9._$]+)\s*:\s*new\s*/\1/m,method/
--regex-js=/this\.([A-Za-z0-9._$]+)\s*=\s*new\s*/\1/p,property/
--regex-js=/([A-Za-z0-9._$]+)\s*=\s*new\s+\(/\1/o,object/
--regex-js=/([A-Za-z0-9._$]+)\s*=\s*([A-Za-z0-9._'"\$\#]+)\.extend\s*\(/\1/c,class/
